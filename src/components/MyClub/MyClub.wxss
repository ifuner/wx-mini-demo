.my-club-container{
    padding: 30rpx 30rpx 180rpx 30rpx;
    box-sizing: border-box;
}
.header {
    color:rgba(249,249,249,1);
    position: relative;
}
.ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.header .user-name{
    font-size:48rpx;
    font-weight:600;
    line-height:67rpx;
    min-height:67rpx;
    max-width: 430rpx;
}
.header .user-desc{
    font-weight:400;
    line-height:33rpx;
    font-size:24rpx;
    min-height:33rpx;
    max-width: 430rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
    overflow: hidden;
}
.header .button-group{
    margin-top: 36rpx;
}
.button-group .share,.button-group .edit-msg {
    color:rgba(83,237,255,1);
    font-weight:500;
    font-size: 28rpx;
    display: inline-block;
    background: transparent;
    padding-left: 50rpx;
}
.button-group button::after{
    border:none;
    border-radius:0;
    transform:scale(1);
    top: 50%;
    transform: translateY(-50%);
    content:"";
    background-size: 100% 100%!important;
    width: 36rpx;
    height: 36rpx;
}
.share::after{
    background: url("https://g.baojiesports.com/bps/d66d290e7e5e49b88340ea74916f205f-36-36.png") 0 0 no-repeat;
}
.edit-msg::after{
    background: url("https://g.baojiesports.com/bps/fc519d01dbe441ea8c946dae1ddc320e-36-36.png") 0 0 no-repeat;
}
.header .user-img{
    width:192rpx;
    height:192rpx;
    position: absolute;
    right: 0;
    top: 0;
    box-shadow:0 2rpx 4rpx 1rpx rgba(255,78,135,0.4);
    border:3rpx solid rgba(255,78,135,1);
}

.header .user-img image{
    width: 100%;
    height: 100%;
    display: block;
}

.user-img-inner {
    width: 100%;
    height: 100%;
    background-color: #fff;
    position: relative;
    z-index: 3;
}
/*个人收益*/
.money{
    margin-top: 45rpx;
    width: 100%;
    height: 126rpx;
    background: url("https://g.baojiesports.com/bps/e0529b2f57e8409897719b4ff3ade133-690-126.png") 0 0 no-repeat;
    -webkit-background-size: cover;
    background-size: cover;
    display: flex;
    justify-content: space-around;
    padding: 0 12rpx;
    box-sizing: border-box;
}
.money .box-li {
    width: 25%;
    padding: 12rpx 0;
    text-align: center;
    box-sizing: border-box;
}
.box-li .box-li-top{
    padding:0 25rpx;
    box-sizing: border-box;
}
.box-li .num{
    font-size:50rpx;
    color:rgba(249,249,249,1);
    line-height:61rpx;
    margin-top: 5rpx;
    font-family:BebasNeueBold;
}
.box-li .line{
    width:100%;
    height:1rpx;
    background:rgba(255,255,255,0.2);
}
.box-li .num-tips{
    font-size:18rpx;
    line-height:25rpx;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(255,255,255,0.6);
    margin-top: 7rpx;
}
/*俱乐部*/
.club-bar{
    height: 88rpx;
    line-height:88rpx;
    display: flex;
    justify-content: space-between;
    margin-top: 30rpx;
    position: relative;
}
.club-bar::before{
    content:"";
    width:4rpx;
    height:24rpx;
    position: absolute;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
    background:rgba(249,249,249,1);
}
.club-bar .left-title{
    font-weight:600;
    color:rgba(249,249,249,1);
    font-size:28rpx;
    padding-left: 20rpx;
}

.club-bar .edit::after{
    border:none;
    transform: scale(1);
}

.club-bar .edit{
    display: inline-block;
    background-color: transparent;
    font-weight:400;
    font-size:28rpx;
    line-height:88rpx;
    color:rgba(83,237,255,1);
    margin: 0;
    padding-right: 0;
}


/* 滚动区域 */
.scroll-user-container{
    width: 100%;
    margin-top: 30rpx;
}
.scroll-inner-user{
    display: flex;
    width: 100%;
    flex-wrap: wrap;
    justify-content: space-between;
}
.scroll-user-container .user-box{
    width:320rpx;
    border-style: solid;
    border-width: 2rpx;
    border-image:linear-gradient(360deg, rgba(56,235,255,0.11), rgba(5,185,255,1)) 2 2;
    filter:blur(0);
    background-color: #262626;
    margin-bottom: 30rpx;
}

.user-box .user-img-other{
    width: 320rpx;
    display: block;
    height: 320rpx;
    background-color: #262626;
}
.fixed-tips{
    font-size:24rpx;
    font-weight:500;
    color:#A5A5A5;
    line-height:33rpx;
    text-align: center;
    width: 100%;
    margin: 50rpx auto;
}
.user-box .user-msg-box{
    height: 130rpx;
    overflow: hidden;
    background:linear-gradient(180deg,rgba(43,43,43,1) 0%,rgba(24,24,24,1) 100%);
    filter:blur(0);
    position: relative;
    color:rgba(255,255,255,1);
    padding: 20rpx;
    box-sizing: border-box;
}
.user-msg-box .user-name{
    font-size:28rpx;
    line-height:40rpx;
    font-weight:500;
    display: flex;
    align-items: center;
}
.user-msg-box .user-name .user-name-text{
    max-width: 60%;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.user-name .tag{
    padding: 0 10rpx;
    height:24rpx;
    line-height: 24rpx;
    text-align: center;
    background:rgba(255,23,81,1);
    border-radius:12rpx;
    font-size:14rpx;
    margin-left: 10rpx;
    font-weight: 600;
    display: flex;
    justify-content: center;
    align-items: center;
    box-sizing: border-box;
}

.tag.baoji{
    background: #FD216F;
}
.tag.baoniang{
    background: #FF4EC3;
}
.user-msg-box .money-data{
    font-size:20rpx;
    line-height: 35rpx;
    margin-top: 15rpx;
}
.user-msg-box .order-btn{
    width: 116rpx;
    height: 42rpx;
    background: url("https://g.baojiesports.com/bps/81c6acfd198b42c5a3c012bdd9d8de3d-116-42.png") 0 0 no-repeat;
    -webkit-background-size: cover;
    background-size: cover;
    position: absolute;
    right: 20rpx;
    bottom: 20rpx;
    font-size:18rpx;
    color:#53EDFF;
    text-align: center;
    line-height: 42rpx;
}

/* 创建我得俱乐部 */

.create-btn{
    width: 480rpx;
    height: 90rpx;
    line-height: 90rpx;
    font-size:32rpx;
    position: fixed;
    bottom: 82rpx;
    left: 50%;
    transform: translateX(-50%);
    font-weight:600;
    color:rgba(24,24,24,1);
    z-index: 10;
    text-align: center;
    background: url("https://g.baojiesports.com/bps/8fac9ae398ca4d04aa362d7c08346861-480-90.png") 0 0 no-repeat;
    -webkit-background-size: cover;
    background-size: cover;
}
.create-btn::after{
    transform: scale(1);
    border:none;
    border-radius: 0;
}
.btn-hover-class {
    transform: translateX(-50%) scale(.9);
}
/* 幕后老板 */
.back-boss{
    height:100rpx;
    line-height: 100rpx;
    border:1rpx solid rgba(120,120,120,1);
    border-left: none;
    border-right: none;
    margin-top: 50rpx;
    font-size:28rpx;
    font-weight:500;
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: relative;
}

.back-boss::after{
    content:"";
    position: absolute;
    right: 0;
    top: 50%;
    width: 36rpx;
    height: 36rpx;
    transform: translateY(-50%);
    background: url("https://g.baojiesports.com/bps/239c3eb064084c81af391870b4769fac-36-36.png") 0 0 no-repeat;
    -webkit-background-size: cover;
    background-size: cover;
}
.boss-left{
    color:rgba(249,249,249,1);
}
.boss-right{
    display: flex;
    align-items: center;
}
.boss-right image{
    width:48rpx;
    height:48rpx;
    border:2rpx solid rgba(255,255,255,1);
    border-radius: 50%;
}
.boss-right .boss-name{
    color:rgba(83,237,255,1);
    margin-left: 20rpx;
    margin-right: 36rpx;
}
