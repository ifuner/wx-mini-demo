.first-club{
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100vh;
    display: flex;
    justify-content: center;
    align-items: center;
}
.img-club{
    width:100%;
    height:993rpx;
}
.create-btn{
    width: 480rpx;
    height: 90rpx;
    line-height: 90rpx;
    font-size:32rpx;
    position: fixed;
    bottom: 40rpx;
    left: 50%;
    transform: translateX(-50%);
    font-weight:600;
    color:rgba(24,24,24,1);
    z-index: 10;
    text-align: center;
    background: url("https://g.baojiesports.com/bps/8fac9ae398ca4d04aa362d7c08346861-480-90.png") 0 0 no-repeat;
    -webkit-background-size: cover;
    background-size: cover;
}
.create-btn::after{
    transform: scale(1);
    border:none;
    border-radius: 0;
}
.btn-hover-class {
    transform: translateX(-50%) scale(.95);
}
